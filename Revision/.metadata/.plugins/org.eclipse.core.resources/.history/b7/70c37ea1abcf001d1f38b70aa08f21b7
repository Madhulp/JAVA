package question2;

public class PrimeFactor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		primeFactor(12);
	}
	
	public static void primeFactor(int n) {
		
		for(int i=0; i<n; i++) {
			if(n % i == 0) {
				System.out.println(i + " ");
			}
			else {
				System.out.println("Invalid Number");
			}
		}
	}

}
